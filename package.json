{
	"private": true,
	"type": "module",
	"scripts": {
		"start": "node --enable-source-maps build/server/main.js",
		"prepare": "simple-git-hooks || true",
		"clear": "del-cli build dist storybook-static",
		"storybook": "run-p sb:*",
		"sb:dev": "storybook dev",
		"sb:types": "npm run dev:client-types",
		"build-storybook": "storybook build",
		"dev": "run-p --race dev:*",
		"dev:client": "vite",
		"dev:client-types": "tsc -b src/client --watch --preserveWatchOutput",
		"dev:storybook": "storybook dev",
		"dev:server": "onchange build node_modules -ik -- npm start",
		"dev:server-build": "tsc -b src/server --watch --preserveWatchOutput",
		"dev:prisma": "prisma generate --watch",
		"dev:db": "docker compose up --build",
		"build": "run-s build:*",
		"build:client": "vite build",
		"build:server": "tsc -b src/server",
		"check": "run-s check:*",
		"check:format": "prettier . --check",
		"check:lint": "eslint . --ext .ts,.tsx",
		"test-storybook": "run-p --race test-storybook:*",
		"test-storybook:prepare": "storybook dev --port 22222 --no-open",
		"test-storybook:run": "wait-on tcp:22222 && test-storybook --coverage --url http://127.0.0.1:22222"
	},
	"simple-git-hooks": {
		"pre-commit": "npx lint-staged"
	},
	"lint-staged": {
		"*.{css,json,yml,yaml,md}": [
			"prettier --write"
		],
		"*.{ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		]
	},
	"browserslist": [
		"chrome >= 109",
		"edge >= 112",
		"firefox >= 112",
		"safari >= 15.6",
		"ios >= 15.6",
		"chromeandroid >= 114"
	],
	"dependencies": {
		"@prisma/client": "4.15.0",
		"@simplewebauthn/server": "^7.3.1",
		"@trpc/server": "^10.30.0",
		"cookie": "^0.5.0",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"nodemailer": "^6.9.3",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@emotion/babel-plugin": "^11.11.0",
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@mui/icons-material": "^5.11.16",
		"@mui/material": "^5.13.5",
		"@simplewebauthn/browser": "^7.2.0",
		"@storybook/addon-a11y": "^7.0.22",
		"@storybook/addon-console": "^2.0.0",
		"@storybook/addon-coverage": "^0.0.8",
		"@storybook/addon-essentials": "^7.0.22",
		"@storybook/addon-interactions": "^7.0.22",
		"@storybook/addon-links": "^7.0.22",
		"@storybook/blocks": "^7.0.22",
		"@storybook/jest": "^0.1.0",
		"@storybook/react": "^7.0.22",
		"@storybook/react-vite": "^7.0.22",
		"@storybook/test-runner": "^0.11.0",
		"@storybook/testing-library": "^0.2.0",
		"@tanstack/react-query": "^4.29.13",
		"@trpc/client": "^10.30.0",
		"@trpc/react-query": "^10.30.0",
		"@types/cookie": "^0.5.1",
		"@types/cors": "^2.8.13",
		"@types/node": "^18.12.0",
		"@types/nodemailer": "^6.4.8",
		"@types/react": "^18.2.12",
		"@types/react-dom": "^18.2.5",
		"@typescript-eslint/eslint-plugin": "^5.60.0",
		"@typescript-eslint/parser": "^5.60.0",
		"@vitejs/plugin-react": "^4.0.0",
		"browserslist-to-esbuild": "^1.2.0",
		"chromatic": "^6.19.8",
		"del-cli": "^5.0.0",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-storybook": "^0.6.12",
		"lint-staged": "^13.2.2",
		"modern-normalize": "^2.0.0",
		"npm-run-all": "^4.1.5",
		"onchange": "^7.1.0",
		"prettier": "^2.8.8",
		"prisma": "^4.15.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.13.0",
		"simple-git-hooks": "^2.8.1",
		"storybook": "^7.0.22",
		"tslib": "^2.5.3",
		"typescript": "~5.1.3",
		"vite": "^4.3.9",
		"wait-on": "^7.0.1",
		"zustand": "^4.3.8"
	},
	"engines": {
		"node": "^18.12.0"
	}
}
