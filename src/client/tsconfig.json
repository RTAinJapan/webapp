{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["ESNext", "DOM"],
		"types": ["vite/client"],
		"module": "ESNext",
		"moduleResolution": "bundler",
		"jsx": "react-jsx",
		"noEmit": true,
		"jsxImportSource": "@emotion/react"
	},
	"include": ["./**/*.ts", "./**/*.tsx", "../util/**/*.ts"]
}
